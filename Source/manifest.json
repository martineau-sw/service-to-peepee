{
	"manifest_version": 3,
	"name": "Service as Peepee",
	"version": "1.0",
	"description": "Replaces service as peepee",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}

